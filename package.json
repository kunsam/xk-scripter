{
  "name": "xk-scripter",
  "version": "0.0.41",
  "description": "",
  "main": "lib/index.js",
  "engine": {
    "node": ">=5.1"
  },
  "scripts": {
    "reset": "npm run build && git add . && git commit -m reset &&npm run publish:patch",
    "start": "npm run build:watch",
    "build": "npm run clean && tsc -p tsconfig.json",
    "clean": "rimraf lib",
    "publish:patch": "npm version patch && npm publish",
    "publish:minor": "npm version minor && npm publish",
    "dot": "NODE_ENV=node npx babel src/cli/create-snippet.js",
    "update-cli": "NODE_ENV=node npx babel scripts/bin/update-cli.js",
    "test-cli": "NODE_ENV=node npx babel-node scripts/bin/test-cli.js"
  },
  "author": "kunsam",
  "license": "MIT",
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "8.3.0",
    "lodash": "4.17.21",
    "readline-sync": "1.4.10",
    "shelljs": "^0.7.8",
    "simple-git": "^2.43.0",
    "xks-git": "0.0.11"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/commander": "^2.12.2",
    "@types/node": "^17.0.8",
    "@types/readline-sync": "^1.4.4",
    "@types/shelljs": "^0.8.10",
    "rimraf": "^2.6.2",
    "typescript": "4.5.4"
  },
  "bin": {
    "XKS": "bin/core.js"
  }
}
